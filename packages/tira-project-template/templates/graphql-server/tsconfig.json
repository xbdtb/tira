{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "lib": ["es2017", "esnext.asynciterable"],
    "types": ["node"],
    "composite": true,
    "rootDir": ".",
    "outDir": "./dist"
  },
  "include": ["server"]
}
